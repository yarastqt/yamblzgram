<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yamblz.yamblzgram" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Yamblzgram</name>
    <content src="index.html" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="ios">
        <icon height="57" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="57" />
        <icon height="114" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="114" />
        <icon height="40" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="40" />
        <icon height="80" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="80" />
        <icon height="50" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="50" />
        <icon height="100" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="100" />
        <icon height="60" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="60" />
        <icon height="120" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="120" />
        <icon height="180" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="180" />
        <icon height="72" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="72" />
        <icon height="144" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="144" />
        <icon height="76" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="76" />
        <icon height="152" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="152" />
        <icon height="29" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="29" />
        <icon height="58" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="58" />
        <icon height="87" platform="ios" src="resources/icon/ios/icon-60@3x.png" width="87" />
        <splash height="960" platform="ios" src="resources/screen/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="resources/screen/ios/Default@2x~iphone.png" width="320" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="resources/icon/android/drawable-xxxhdpi-icon.png" />
        <icon density="mdpi" src="resources/icon/android/drawable-xxxhdpi-icon.png" />
        <icon density="hdpi" src="resources/icon/android/drawable-xxxhdpi-icon.png" />
        <icon density="xhdpi" src="resources/icon/android/drawable-xxxhdpi-icon.png" />
        <icon density="xxhdpi" src="resources/icon/android/drawable-xxxhdpi-icon.png" />
        <icon density="xxxhdpi" src="resources/icon/android/drawable-xxxhdpi-icon.png" />
        <splash density="port-ldpi" src="resources/screen/android/drawable-port-hdpi-screen.png" />
        <splash density="port-mdpi" src="resources/screen/android/drawable-port-hdpi-screen.png" />
        <splash density="port-hdpi" src="resources/screen/android/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="resources/screen/android/drawable-port-hdpi-screen.png" />
        <splash density="port-xxhdpi" src="resources/screen/android/drawable-port-hdpi-screen.png" />
        <splash density="port-xxxhdpi" src="resources/screen/android/drawable-port-hdpi-screen.png" />
    </platform>
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-actionsheet" spec="^2.3.3" />
    <plugin name="cordova-plugin-camera" spec="^2.4.1">
        <variable name="CAMERA_USAGE_DESCRIPTION" value=" " />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-device" spec="^1.1.6" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.3" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-media-capture" spec="^1.4.3">
        <variable name="CAMERA_USAGE_DESCRIPTION" value=" " />
        <variable name="MICROPHONE_USAGE_DESCRIPTION" value=" " />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-photo-library" spec="^2.0.4">
        <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-splashscreen" spec="^3.2.2" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
